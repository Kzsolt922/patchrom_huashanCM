<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabet|alphabetAutomaticShifted">
            <Key latin:moreKeys="&lt;" latin:keyLabel="," />
            <Key latin:moreKeys=">" latin:keyLabel="." />
            <Key latin:moreKeys="\?,¿" latin:keyLabel="/" />
        </case>
        <default>
            <Key latin:moreKeys="!fixedColumnOrder!3,‹,≤,«" latin:keyLabel="&lt;" />
            <Key latin:moreKeys="!fixedColumnOrder!3,›,≥,»" latin:keyLabel=">" />
            <Key latin:moreKeys="¿" latin:keyLabel="\?" />
        </default>
    </switch>
</merge>