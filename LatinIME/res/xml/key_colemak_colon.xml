<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLockShifted">
            <Key latin:additionalMoreKeys="0" latin:keyLabel=";" latin:keyHintLabel="0" />
        </case>
        <default>
            <Key latin:moreKeys=";" latin:additionalMoreKeys="0" latin:keyLabel=":" latin:keyHintLabel="0" />
        </default>
    </switch>
</merge>