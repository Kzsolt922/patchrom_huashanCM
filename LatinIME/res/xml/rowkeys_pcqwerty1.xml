<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabet|alphabetAutomaticShifted">
            <Key latin:moreKeys="~" latin:additionalMoreKeys="ยฌ" latin:keyLabel="`" />
            <Key latin:moreKeys="ยก,!text/more_keys_for_symbols_1" latin:additionalMoreKeys="!" latin:keyLabel="1" />
            <Key latin:moreKeys="!text/more_keys_for_symbols_2" latin:additionalMoreKeys="\@" latin:keyLabel="2" />
            <Key latin:moreKeys="!text/more_keys_for_symbols_3" latin:additionalMoreKeys="\#" latin:keyLabel="3" />
            <Key latin:moreKeys="!text/more_keys_for_symbols_4" latin:additionalMoreKeys="$" latin:keyLabel="4" />
            <Key latin:moreKeys="!text/more_keys_for_symbols_5" latin:additionalMoreKeys="\\%" latin:keyLabel="5" />
            <Key latin:moreKeys="!text/more_keys_for_symbols_6" latin:additionalMoreKeys="^" latin:keyLabel="6" />
            <Key latin:moreKeys="!text/more_keys_for_symbols_7" latin:additionalMoreKeys="&amp;" latin:keyLabel="7" />
            <Key latin:moreKeys="!text/more_keys_for_symbols_8" latin:additionalMoreKeys="*,%" latin:keyLabel="8" />
            <Key latin:moreKeys="!text/more_keys_for_symbols_9" latin:additionalMoreKeys="(" latin:keyLabel="9" />
            <Key latin:moreKeys="!text/more_keys_for_symbols_0" latin:additionalMoreKeys=")" latin:keyLabel="0" />
            <Key latin:moreKeys="_" latin:keyLabel="-" />
            <Key latin:moreKeys="+" latin:keyLabel="=" />
        </case>
        <default>
            <include latin:keyboardLayout="@xml/keys_pcqwerty_symbols1" />
        </default>
    </switch>
</merge>