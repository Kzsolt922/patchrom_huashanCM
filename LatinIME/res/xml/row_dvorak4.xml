<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <Row latin:keyWidth="10.000002%p">
        <Key latin:keyStyle="toSymbolKeyStyle" latin:keyWidth="14.999998%p" />
        <switch>
            <case latin:keyboardLayoutSetElement="alphabet" latin:hasShortcutKey="true">
                <Key latin:additionalMoreKeys="!text/shortcut_as_more_key" latin:backgroundType="normal" latin:keyLabel="q" latin:keyStyle="f1MoreKeysStyle" />
            </case>
            <case latin:hasShortcutKey="true">
                <Key latin:additionalMoreKeys="!text/shortcut_as_more_key" latin:backgroundType="normal" latin:keyLabel="Q" latin:keyStyle="f1MoreKeysStyle" />
            </case>
            <case latin:keyboardLayoutSetElement="alphabet">
                <Key latin:backgroundType="normal" latin:keyLabel="q" latin:keyStyle="f1MoreKeysStyle" />
            </case>
            <default>
                <Key latin:backgroundType="normal" latin:keyLabel="Q" latin:keyStyle="f1MoreKeysStyle" />
            </default>
        </switch>
        <include latin:keyXPos="25.0%p" latin:keyboardLayout="@xml/key_space" />
        <Key latin:moreKeys="!text/more_keys_for_z" latin:keyLabel="z" />
        <Key latin:keyStyle="enterKeyStyle" latin:keyWidth="fillRight" />
    </Row>
</merge>