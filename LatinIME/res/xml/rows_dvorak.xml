<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <include latin:keyboardLayout="@xml/key_styles_common" />
    <Row latin:keyWidth="10.000002%p">
        <include latin:keyboardLayout="@xml/keys_dvorak_123" />
        <include latin:keyboardLayout="@xml/rowkeys_dvorak1" />
    </Row>
    <Row latin:keyWidth="10.000002%p">
        <include latin:keyboardLayout="@xml/rowkeys_dvorak2" />
    </Row>
    <Row latin:keyWidth="10.000002%p">
        <Key latin:keyStyle="shiftKeyStyle" latin:visualInsetsRight="0.99999905%p" latin:keyWidth="14.999998%p" />
        <include latin:keyboardLayout="@xml/rowkeys_dvorak3" />
        <Key latin:keyStyle="deleteKeyStyle" latin:visualInsetsLeft="0.99999905%p" latin:keyWidth="fillRight" />
    </Row>
    <include latin:keyboardLayout="@xml/row_dvorak4" />
</merge>