<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:mode="url">
            <Key latin:keyLabel="/" latin:keyStyle="f1MoreKeysStyle" />
        </case>
        <case latin:mode="email">
            <Key latin:keyLabel="\@" latin:keyStyle="f1MoreKeysStyle" />
        </case>
        <case latin:hasShortcutKey="true">
            <Key latin:keyStyle="shortcutKeyStyle" />
        </case>
        <default>
            <Key latin:additionalMoreKeys="!text/more_keys_for_comma" latin:keyLabel="!text/keylabel_for_comma" latin:keyLabelFlags="hasPopupHint" latin:keyStyle="f1MoreKeysStyle" />
        </default>
    </switch>
</merge>