<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <include latin:keyboardLayout="@xml/keys_less_greater" />
    <Key latin:moreKeys="≠,≈" latin:keyLabel="=" />
    <switch>
        <case latin:mode="url">
            <Key latin:keyLabel="'" />
        </case>
        <default>
            <Key latin:keyLabel=":" />
        </default>
    </switch>
    <Key latin:moreKeys="!text/more_keys_for_symbols_semicolon" latin:keyLabel="!text/keylabel_for_symbols_semicolon" />
    <Key latin:moreKeys="!text/more_keys_for_comma" latin:keyLabel="!text/keylabel_for_comma" />
    <Key latin:keyLabel="." />
    <switch>
        <case latin:languageCode="hy">
            <Key latin:moreKeys="!text/more_keys_for_symbols_exclamation" latin:keyLabel="՛" latin:keyHintLabel="՜" />
        </case>
        <default>
            <Key latin:moreKeys="!text/more_keys_for_symbols_exclamation" latin:keyLabel="!" />
        </default>
    </switch>
    <switch>
        <case latin:languageCode="hy">
            <Key latin:moreKeys="!text/more_keys_for_symbols_question" latin:keyLabel="՞" latin:keyHintLabel="՝" />
        </case>
        <default>
            <Key latin:moreKeys="!text/more_keys_for_symbols_question" latin:keyLabel="!text/keylabel_for_symbols_question" />
        </default>
    </switch>
</merge>