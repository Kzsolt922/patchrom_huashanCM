<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:mode="email|url">
            <Key latin:keyLabel="-" />
        </case>
        <case latin:languageCode="fa">
            <Key latin:moreKeys="!text/more_keys_for_apostrophe" latin:keyLabel="!text/keylabel_for_apostrophe" latin:keyHintLabel="!text/keyhintlabel_for_apostrophe" latin:keyLabelFlags="hasPopupHint" latin:keyStyle="hasShiftedLetterHintStyle" />
        </case>
        <default>
            <Key latin:moreKeys="!text/more_keys_for_apostrophe" latin:keyLabel="!text/keylabel_for_apostrophe" latin:keyHintLabel="!text/keyhintlabel_for_apostrophe" latin:keyStyle="hasShiftedLetterHintStyle" />
        </default>
    </switch>
</merge>