<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <include latin:keyboardLayout="@xml/key_styles_common" />
    <Row latin:keyWidth="9.000004%p">
        <include latin:keyboardLayout="@xml/key_apostrophe" />
        <include latin:keyboardLayout="@xml/key_dash" />
        <include latin:keyboardLayout="@xml/rowkeys_hebrew1" />
        <Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyWidth="9.000004%p">
        <include latin:keyboardLayout="@xml/rowkeys_hebrew2" />
        <Key latin:keyStyle="enterKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyWidth="9.000004%p">
        <include latin:keyXPos="10.000002%p" latin:keyboardLayout="@xml/rowkeys_hebrew3" />
    </Row>
    <include latin:keyboardLayout="@xml/row_hebrew4" />
</merge>