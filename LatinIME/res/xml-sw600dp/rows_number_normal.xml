<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <Row>
        <Key latin:backgroundType="functional" latin:keyLabel="-" latin:keyStyle="numKeyStyle" latin:keyWidth="10.000002%p" />
        <Key latin:backgroundType="functional" latin:keyLabel="+" latin:keyStyle="numKeyStyle" latin:keyWidth="10.000002%p" />
        <Key latin:backgroundType="functional" latin:keyLabel="." latin:keyStyle="numKeyStyle" latin:keyWidth="10.000002%p" />
        <Key latin:keyLabel="1" latin:keyStyle="numKeyStyle" latin:keyXPos="31.000006%p" />
        <Key latin:keyLabel="2" latin:keyStyle="numKeyStyle" />
        <Key latin:keyLabel="3" latin:keyStyle="numKeyStyle" />
        <Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillRight" latin:keyXPos="-13.999998%p" />
    </Row>
    <Row>
        <Key latin:backgroundType="functional" latin:keyStyle="numStarKeyStyle" latin:keyWidth="10.000002%p" />
        <Key latin:backgroundType="functional" latin:keyLabel="/" latin:keyStyle="numKeyStyle" latin:keyWidth="10.000002%p" />
        <switch>
            <case latin:mode="time|datetime">
                <Key latin:moreKeys="!text/more_keys_for_am_pm" latin:backgroundType="functional" latin:keyLabel="," latin:keyLabelFlags="hasPopupHint" latin:keyStyle="numKeyStyle" latin:keyWidth="10.000002%p" />
            </case>
            <default>
                <Key latin:backgroundType="functional" latin:keyLabel="," latin:keyStyle="numKeyStyle" latin:keyWidth="10.000002%p" />
            </default>
        </switch>
        <Key latin:keyLabel="4" latin:keyStyle="numKeyStyle" latin:keyXPos="31.000006%p" />
        <Key latin:keyLabel="5" latin:keyStyle="numKeyStyle" />
        <Key latin:keyLabel="6" latin:keyStyle="numKeyStyle" />
        <Key latin:keyStyle="enterKeyStyle" latin:keyWidth="fillRight" latin:keyXPos="-13.999998%p" />
    </Row>
    <Row>
        <Key latin:backgroundType="functional" latin:keyLabel="(" latin:keyStyle="numKeyStyle" latin:keyWidth="10.000002%p" />
        <Key latin:backgroundType="functional" latin:keyLabel=")" latin:keyStyle="numKeyStyle" latin:keyWidth="10.000002%p" />
        <switch>
            <case latin:mode="time|datetime">
                <Key latin:backgroundType="functional" latin:keyLabel=":" latin:keyStyle="numKeyStyle" latin:keyWidth="10.000002%p" />
            </case>
            <default>
                <Key latin:backgroundType="functional" latin:keyLabel="=" latin:keyStyle="numKeyStyle" latin:keyWidth="10.000002%p" />
            </default>
        </switch>
        <Key latin:keyLabel="7" latin:keyStyle="numKeyStyle" latin:keyXPos="31.000006%p" />
        <Key latin:keyLabel="8" latin:keyStyle="numKeyStyle" />
        <Key latin:keyLabel="9" latin:keyStyle="numKeyStyle" />
        <Spacer />
    </Row>
    <Row>
        <Key latin:backgroundType="functional" latin:keyStyle="spaceKeyStyle" latin:keyWidth="29.999996%p" />
        <Key latin:keyStyle="numStarKeyStyle" latin:keyXPos="31.000006%p" />
        <Key latin:keyLabel="0" latin:keyStyle="numKeyStyle" />
        <Key latin:keyLabel="\#" latin:keyStyle="numKeyStyle" />
        <Spacer latin:keyWidth="fillRight" />
    </Row>
</merge>